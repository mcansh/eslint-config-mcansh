{
  "name": "@mcansh/eslint-config",
  "version": "3.2.0",
  "private": false,
  "description": "my usual eslint config",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules eslint-check",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "eslint-check": "npm-run-all --parallel eslint-check:*",
    "eslint-check:base": "npx eslint-config-prettier --print-config ./base.js",
    "eslint-check:main": "npx eslint-config-prettier --print-config ./index.js",
    "eslint-check:ts": "npx eslint-config-prettier --print-config ./typescript.js",
    "validate": "npm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.16.3",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-kentcdodds": "^19.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "read-pkg-up": "^7.0.0"
  },
  "devDependencies": {
    "@types/prettier": "2.4.2",
    "eslint": "7.32.0",
    "eslint-find-rules": "4.0.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "eslint": "7.32.0"
  },
  "optionalDependencies": {
    "@next/eslint-plugin-next": "^10.2.0 || ^11.0.0 || ^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^2.0.0"
  }
}
