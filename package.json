{
  "name": "@mcansh/eslint-config",
  "version": "3.1.0-canary.0",
  "private": false,
  "description": "my usual eslint config",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules eslint-check",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "eslint-check": "npm-run-all --parallel eslint-check:*",
    "eslint-check:base": "npx eslint-config-prettier --print-config ./base.js",
    "eslint-check:main": "npx eslint-config-prettier --print-config ./index.js",
    "eslint-check:ts": "npx eslint-config-prettier --print-config ./typescript.js",
    "validate": "npm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.14.3",
    "@next/eslint-plugin-next": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-kentcdodds": "^18.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.11.0",
    "read-pkg-up": "^7.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-find-rules": "3.6.1",
    "husky": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "eslint": "^7.22.0"
  }
}
