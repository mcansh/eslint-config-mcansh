{
  "name": "@mcansh/eslint-config",
  "version": "3.1.0-canary.0",
  "private": false,
  "description": "my usual eslint config",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules eslint-check",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "eslint-check": "npm-run-all --parallel eslint-check:*",
    "eslint-check:base": "eslint-config-prettier ./base.js",
    "eslint-check:main": "eslint-config-prettier ./index.js",
    "eslint-check:ts": "eslint-config-prettier ./typescript.js",
    "validate": "npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^10.0.5",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-kentcdodds": "^17.3.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "read-pkg-up": "^7.0.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.2"
  },
  "devDependencies": {
    "eslint": "7.12.1",
    "eslint-find-rules": "3.6.1",
    "husky": "4.3.8",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "eslint": "^7.18.0"
  }
}
