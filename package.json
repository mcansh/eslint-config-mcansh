{
  "name": "@mcansh/eslint-config",
  "version": "2.5.1",
  "description": "my usual eslint config",
  "main": "index.js",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules eslint-check",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "eslint-check": "npm-run-all --parallel eslint-check:*",
    "eslint-check:base": "eslint --print-config ./base.js | eslint-config-prettier-check",
    "eslint-check:main": "eslint --print-config ./index.js | eslint-config-prettier-check",
    "eslint-check:ts": "eslint --print-config ./typescript.js | eslint-config-prettier-check",
    "validate": "yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn validate"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-kentcdodds": "14.7.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-find-rules": "3.4.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.9.0",
    "husky": "4.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "stylelint": "13.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-prettier": "1.1.2",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-eslint": ">=10",
    "eslint": ">=5",
    "eslint-config-airbnb": ">=18",
    "eslint-config-airbnb-base": ">=14",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-html": ">=6",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.8.0",
    "prettier": "^1.18.2",
    "stylelint": ">=11",
    "stylelint-config-prettier": ">=6",
    "stylelint-config-recommended": ">=3",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.6.3"
  }
}
