{
  "name": "eslint-config-mcansh",
  "version": "2.0.0-canary.0",
  "description": "my usual eslint config",
  "main": "index.js",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "validate": "npm t"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-kentcdodds": "^14.4.0",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "read-pkg-up": "^6.0.0",
    "semver": "^6.3.0"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "@mcansh/prettier-config": "1.0.0",
    "eslint": "^6.2.1",
    "eslint-find-rules": "^3.4.0",
    "husky": "3.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "typescript": "^3.5.3"
  }
}
