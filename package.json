{
  "name": "@mcansh/eslint-config",
  "version": "2.1.2",
  "description": "my usual eslint config",
  "main": "index.js",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "files": [
    "base.js",
    "common.js",
    "index.js",
    "typescript.js",
    "prettier.config.js"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules && npm-run-all --parallel lint eslint-check",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "eslint-check": "npm-run-all --parallel eslint-check:*",
    "eslint-check:base": "eslint --print-config ./base.js | eslint-config-prettier-check",
    "eslint-check:main": "eslint --print-config ./index.js | eslint-config-prettier-check",
    "eslint-check:ts": "eslint --print-config ./typescript.js | eslint-config-prettier-check",
    "validate": "npm t"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "devDependencies": {
    "@mcansh/prettier-config": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-kentcdodds": "^14.4.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "3.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "@mcansh/prettier-config": "1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "1.18.2"
  }
}
