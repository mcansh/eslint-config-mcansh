{
  "name": "eslint-config-mcansh",
  "version": "1.2.0",
  "description": "my usual eslint config",
  "main": "index.js",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "precommit": "npm run validate",
    "validate": "npm t"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-kentcdodds": "^14.3.3",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "prettier": "^1.18.2",
    "read-pkg-up": "^6.0.0",
    "semver": "^6.2.0"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-find-rules": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3"
  }
}
