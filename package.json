{
  "name": "eslint-config-mcansh",
  "version": "1.1.0-canary.5",
  "description": "my usual eslint config",
  "main": "index.js",
  "repository": "https://github.com/mcansh/eslint-config-mcansh",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "test": "npm-run-all --parallel lint find-new-rules",
    "find-new-rules": "npm-run-all --parallel find-new-rules:*",
    "find-new-rules:base": "eslint-find-rules --unused ./base.js",
    "find-new-rules:main": "eslint-find-rules --unused ./index.js",
    "find-new-rules:ts": "eslint-find-rules --unused ./typescript.js",
    "precommit": "npm run validate",
    "validate": "npm t"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-kentcdodds": "^14.2.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "prettier": "^1.16.4",
    "read-pkg-up": "^4.0.0",
    "semver": "^5.6.0"
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-find-rules": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.3.3333"
  }
}
